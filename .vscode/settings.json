{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.rulers": [
    80
  ],
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "cSpell.words": [
    "Alore",
    "Kleene",
    "OOLs",
    "Plock",
    "actuals",
    "cadr",
    "callees",
    "cdecl",
    "datatypes",
    "deallocation",
    "defragmentation",
    "fastcall",
    "memcpy",
    "printf",
    "stdcall",
    "subclassing",
    "subtyping",
    "thiscall",
    "vtable",
    "vuepress"
  ],
}
